{
  "name": "ExoCreate",
  "version": "1.0.0",
  "description": "ExoCreate itinerary builder based on React, Relay, GraphQL, Express, ES6/ES7, JSX, Webpack, Babel, Materialize, and PostCSS",
  "scripts": {
    "start": "nodemon --watch 'server/data/**/*.js' --exec 'npm run update && babel-node server/index.js'",
    "debug": "npm run update && babel-node server/index.js",
    "start-prod": "npm run update && babel-node server/index.js",
    "start:production": "NODE_ENV=production node build-server/index.js",
    "build": "concurrently --no-color 'npm run build:server' 'npm run build:client'",
    "build:client": "webpack --config webpack.production.config.js",
    "build:server": "babel -d build-server server",
    "deploy": "npm run build && npm run start:production",
    "update": "babel-node server/utils/updateSchema.js",
    "lint": "eslint client server test --fix --ext .js --ext .jsx --quiet",
    "lintChangedFiles": "./eslintModifiedFiles.sh",
    "restore": "arangorestore --server.database $DB --server.username $USER --input-directory $DIR",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "mocha --require test/withDom.js --compilers js:babel-register --require babel-polyfill --require ignore-styles --reporter list --recursive  $npm_package_config_testExtensions",
    "tdd": "npm run test -- -w",
    "postinstall": "cp node_modules/materialize-css/js/date_picker/picker.js node_modules/materialize-css/bin"
  },
  "config": {
    "testExtensions": "test/**/*.@(js|jsx)"
  },
  "precommit": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uncircled/quickplan.git"
  },
  "keywords": [
    "react",
    "relay",
    "graphql",
    "express",
    "es6",
    "es7",
    "jxs",
    "webpack",
    "babel",
    "material design lite",
    "postcss"
  ],
  "author": "Uncircled Solutions",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uncircled/quickplan/issues"
  },
  "homepage": "https://github.com/uncircled/quickplan",
  "devDependencies": {
    "@kadira/storybook": "^2.5.2",
    "cheerio": "^0.22.0",
    "eslint": "^3.9.1",
