{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "globals": {
    "conApp": false,
    "describe": true,
    "it": true,
    "before": true
  },
  "rules": {
    "generator-star-spacing": 0,
    "comma-dangle": 0,
    "jsx-quotes": [2, "prefer-single"],
    "no-underscore-dangle": 0,
    "no-use-before-define": 1,
    "no-unused-vars": 1,
    "no-undef": 1,
    "max-len": 1,
    "camelcase": 1,
    "react/prefer-stateless-function": 1,
    "react/sort-comp": 1
  }
}
